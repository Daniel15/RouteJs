{
  "version": "2.2.0-*",
  "title": "RouteJs (ASP.NET Core MVC)",
  "authors": [ "Daniel Lo Nigro" ],

  "description": "JavaScript URL routing for ASP.NET Core (vNext) and MVC 6. Allows you to use your ASP.NET MVC routes from JavaScript. Please refer to project site (http://dan.cx/projects/routejs) for more details, usage examples and sample code.",

  "dependencies": {
    "Microsoft.AspNetCore.Html.Abstractions": "1.1.0",
    "Microsoft.AspNetCore.Http": "1.1.0",
    "Microsoft.AspNetCore.Mvc": "1.1.0",
    "Microsoft.AspNetCore.Routing": "1.1.0"
  },
  "frameworks": {
    "net451": {
      "bin": {
        "assembly": "../../bin/RouteJs.AspNet/bin/{configuration}/net451/RouteJs.AspNet.dll",
        "pdb": "../../bin/RouteJs.AspNet/bin/{configuration}/net451/RouteJs.AspNet.pdb"
      }
    },
    "netstandard1.6": {
      "imports": [
        "dnxcore50"
      ],
      "bin": {
        "assembly": "../../bin/RouteJs.AspNet/bin/{configuration}/netstandard1.6/RouteJs.AspNet.dll",
        "pdb": "../../bin/RouteJs.AspNet/bin/{configuration}/netstandard1.6/RouteJs.AspNet.pdb"
      }
    }
  },
  "buildOptions": {
    "compile": [
      "../SharedAssemblyVersionInfo.cs"
    ],
    "xmlDoc": true,
    "nowarn": [ "CS7035" ],
    "warningsAsErrors": true
  },
  "packOptions": {
    "owners": [ "Daniel Lo Nigro" ],
    "licenseUrl": "http://dan.cx/projects/routejs#licence",
    "projectUrl": "http://dan.cx/projects/routejs",
    "requireLicenseAcceptance": false,
    "releaseNotes": "Please refer to project site (http://dan.cx/projects/routejs) for more details, release notes, usage examples and sample code.",
    "tags": [ "asp.net", "mvc", "asp", "routing", "route", "url", "ajax", "jquery", "javascript", "js", "vnext", "asp.net 5" ],
    "repository": {
      "type": "git",
      "url": "git://github.com/Daniel15/RouteJs"
    }
  }
}
